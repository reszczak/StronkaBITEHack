{
  "name": "gatsby-plugin-polyfill-io",
  "version": "1.1.0",
  "keywords": [
    "gatsby-plugin",
    "gatsby",
    "plugin",
    "polyfill",
    "polyfill.io"
  ],
  "main": "gatsby-ssr.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/gatsby-plugin-polyfill-io.git"
  },
  "license": "MIT",
  "scripts": {
